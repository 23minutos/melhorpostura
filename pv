			<script>
    var $ = jQuery.noConflict();
    
    var checkout = {
     kitUm: 'https://go.perfectpay.com.br/PPU38CK606A',
     kitDois: 'https://go.perfectpay.com.br/PPU38CK606B',
     kitTres: 'https://go.perfectpay.com.br/PPU38CK606C'
    }
 
    $(function(){
        var url_string = window.location.href;
        var urlparams = new URL(url_string);
        var src = urlparams.searchParams.get("src");
        
        var kit1 = checkout.kitUm + '?split=12&src=' + src;
        var kit2 = checkout.kitDois + '?split=12&src=' + src;
        var kit3 = checkout.kitTres + '?split=12&src=' + src;
    
        $(".kitUm").attr('href', kit1).attr('target', '_blank');
        $(".kitDois").attr('href', kit2).attr('target', '_blank');
        $(".kitTres").attr('href', kit3).attr('target', '_blank');
        
    });
</script>
